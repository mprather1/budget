


<table>
  <tr>
    <th>
      Account Number
    </th>
    <th>
      Balance
    </th>
  </tr>
  <% @accounts.each do |account| %>
  <tr>
    <td>
      <%= link_to account.account_number, account_path(account) %>
    </td>
    <td>
      <%= number_to_currency(account.balance) %>
    </td>
    <% end %>
  </tr>
</table>
