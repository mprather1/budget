


<table>
  <tr>
    <th>
      Account Number
    </th>
    <th>
      Balance
    </th>
  </tr>
  <% @accounts.each do |account| %>
  <tr>
    <td>
      <%= account.account_number %><br>
    </td>
    <td>
      <%= number_to_currency(account.balance) %><br>
    </td>
    <% end %>
  </tr>
</table>
